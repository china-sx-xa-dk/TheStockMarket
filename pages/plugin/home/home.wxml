<scroll-view scroll-y class="scrollPage">
  <!-- <cu-custom bgImage="/images/cjkz2329.jpg"> -->
  <!-- <cu-custom bgImage="https://image.weilanwl.com/color2.0/plugin/cjkz2329.jpg"> -->
  <cu-custom bgImage="https://aitools.oss-cn-beijing.aliyuncs.com/customNavigationBar.jpg">
    <view slot="content">
      <image src="/images/cjkz001.png" mode="widthFix"></image>
    </view>
  </cu-custom>
  <view class="cu-card">
    <view class="cu-item bg-img shadow-blur" style="background-image:url({{item.img}})" bindtap="toChild" data-url="{{item.url}}" wx:for="{{list}}" wx:key="index">
      <view class="cardTitle">
        {{item.title}}
      </view>
    </view>
  </view>
  <view class="cu-tabbar-height"></view>
</scroll-view>
